[zbxCredenials]
; mandatory
user = user
; optional, if omitted you will be ask for password
passwd = password
; mandatory
host = host.example.com
; mandatory
url = https://host.example.com/zabbix

[zbxOptions]
; all opions in this section are optional. Defaults:
; ackOnly = True
; icon = host[-ok.png|-err.png]
; ignore_warn = False
; interval = 30[s]
; log_truncate = False
; notify = True
; port = 10051
; text_mode = False
; wav = None
; wav_player = /usr/bin/mpv
; exclTg = []
; inclTg = []

interval = 30
notify = True
port = 10051
text_mode = False
ignore_warn = True
; icon = host.example.com
icon = icon
wav = sound
wav_player = /usr/bin/mpv
ackOnly = True

; exclTg = []
exclTg = [
    "^SSL.*certificate",
    "Free disk space"
    ]

; inclTg = []
inclTg = [
    ".*\.google\.com"
    ]

